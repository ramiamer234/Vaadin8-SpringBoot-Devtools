<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
         updateCheck="true" monitoring="autodetect" dynamicConfig="true">
	<defaultCache eternal="true" maxElementsInMemory="100" overflowToDisk="false" />
	
	<cache name="country" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
	<cache name="state" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
	<cache name="currency" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
    <cache name="lookup" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />

</ehcache>